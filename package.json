{
  "name": "leaflet-nearmap",
  "description": "Nearmap CRS for leaflet library",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "node": ">=10.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nearmap/leaflet-nearmap.git"
  },
  "bugs": "https://github.com/nearmap/leaflet-nearmap/issues",
  "homepage": "https://github.com/nearmap/leaflet-nearmap",
  "scripts": {
    "default": "run dev-server",
    "dev-server": "webpack-dev-server --context ./examples --config ./examples/webpack.config.babel.js --progress",
    "clean": "rimraf ./build",
    "build": "run build:*",
    "build:babel": "babel src --out-dir build --ignore '**/*.test.js' --ignore '**/testing'",
    "build:files": "cp ./README.md ./LICENSE package.json build/",
    "test": "run lint jest",
    "lint": "run lint:*",
    "lint:js": "eslint --report-unused-disable-directives --ignore-path .gitignore .",
    "lint:md": "remark --no-stdout --use remark-lint *.md",
    "jest": "jest --runInBand --no-cache ",
    "cd": "run clean build release",
    "release": "semantic-release"
  },
  "main": "./index.js",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@nearmap/eslint-config-react": "^1.0.1",
    "@nearmap/jest-config": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "commitizen": "^2.10.1",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.5.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^23.6.0",
    "js-beautify": "^1.8.6",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.2",
    "npx-run": "^2.1.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-entry-loader": "^2.2.4",
    "react-syntax-highlighter": "^8.0.1",
    "remark-cli": "^5.0.0",
    "remark-lint": "^6.0.2",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.14",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/generator": "^7.0.0",
    "@babel/traverse": "^7.1.0",
    "@babel/types": "^7.0.0",
    "leaflet": "^1.3.4",
    "loader-utils": "^1.1.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
